	color:#c02230;
}
.success-msg {
	border-color:#296a23;
	background-color:#e9ffca;
	background-image:url(//prod.az-boutique.fr/media/extendware/ewminify/media/css/1340716764/54/i_msg-success.png);
	color:#296a23;
}
.note-msg,
.notice-msg {
	border-color:#7c622a;
	background-color:#FDEDA4;
	background-image:url(//prod.az-boutique.fr/media/extendware/ewminify/media/css/1340716700/83/i_msg-note.png);
	color:#7c622a;
}


/* BreadCrumbs
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
*/
.breadcrumbs li { display:inline; font-size:0.9166em; /*11px*/ }
.breadcrumbs a { display:block; float:left; }
.breadcrumbs li span {
	background:url(//prod.az-boutique.fr/media/extendware/ewminify/media/css/1370275580/56/pix.png) 0 -129px no-repeat;
	width:7px;
